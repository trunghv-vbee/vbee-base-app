{
  "name": "vbee",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android": "cd android && ./gradlew clean && cd ../ && react-native run-android",
    "ios": "cd ios && xcodebuild clean && cd ../ && react-native run-ios",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleStaging && cd ../",
    "genimg": "node genimg src/assets/images --output src/assets/images.ts --name images",
    "postinstall": "npx patch-package && npx typesync && RCT_NEW_ARCH_ENABLED=0  npx pod-install ios",
    "typesync": "npx typesync",
    "build:StagingAndroid": "cd ./android && fastlane uploadAPK$param envi:Staging",
    "build:StagingIos": "cd ./ios && fastlane uploadIPA$param envi:Staging",
    "genfont": "node genimg src/assets/fonts --output src/assets/fonts.ts --name fonts",
    "pushAppProduction": "sh release-app.sh production",
    "pushAppTest": " sh release-app.sh staging"
  },
